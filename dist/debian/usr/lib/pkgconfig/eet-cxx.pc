prefix=/usr/local/src/enlightenment.source.repo/dist/debian/usr
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: Eet C++
Description: C++ API for the eet library
Version: 1.15.0-beta2
Requires.private: emile >= 1.15.0-beta2 eina >= 1.15.0-beta2 openssl   
Libs: -L${libdir} -leet
Libs.private:   -ljpeg -lm
Cflags: -I${includedir}/efl-1 -I${includedir}/eet-1 -I${includedir}/efl-1 -I${includedir}/eet-cxx-1 -I${includedir}/eet-cxx-1/eet-cxx
